	<bean id="mySmartOnFhirLaunchContext" class="edu.gatech.i3l.fhir.smart.servlet.SmartOnFhirLaunchContextDao">
		<property name="baseFhirDao" ref="myBaseDao"></property>
		<property name="context" ref="myFhirContextDstu2"/>
	</bean>
	<bean id="mySmartService" class="edu.gatech.i3l.fhir.smart.servlet.SmartOnFhirLaunchContextProvider">
		<property name="dao" ref="mySmartOnFhirLaunchContext"/>
	</bean>
	
	
